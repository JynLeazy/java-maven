pipeline {
    agent any
    stages {    
        stage('Checkout Scm') {
            steps {
                git 'https://github.com/JynLeazy/java-maven.git'
            }
        }
        
        stage('javadoc') {
            steps {                
                sh '''
                mkdir - p src/javadoc/
                cd src/javadoc/
                javadoc ../* -subpackages
                javadoc App.java'''
            }
        }        
    }
}
